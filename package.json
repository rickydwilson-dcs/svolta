    "@supabase/supabase-js": "^2.86.0",
    "clsx": "^2.1.1",
    "fabric": "^6.9.0",
    "framer-motion": "^12.23.24",
    "gif.js": "^0.2.0",
    "heic2any": "^0.0.4",
    "next": "^16.0.7",
    "next-themes": "^0.4.6",
