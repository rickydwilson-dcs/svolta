{
  "name": "svolta",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "playwright test --grep @smoke",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "vitest run tests/visual/alignment.visual.test.ts",
    "test:visual:watch": "vitest tests/visual/alignment.visual.test.ts",
    "test:visual:unit": "vitest run tests/visual/alignment.unit.test.ts",
    "test:visual:generate": "npx tsx tests/visual/fixtures/generate-fixtures.ts",
    "test:visual:report": "open tests/visual/report.html",
    "validate": "npm run type-check && npm run lint && npm run test run",
    "prepare": "husky"
  },
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.22-rc.20250304",
